@model tiplLogin.Models.login.regismodel


@{
    ViewBag.Title = "matchotp";
}
<h2>matchotp</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div id="main-container" class="main-container">
        <div id="inner-container" class="inner-container">

            <div id="middle">
                <section class="common-section collegebol-predictor-section">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <h1 class="cmn-heading">Register</h1>
                            </div>

                            <div class="col-md-12">
                                <div class="register-step-cnt">
                                    <div class="register-steps-area">
                                        <ul>
                                            <li class="completed"><span>1</span></li>
                                            <li class="completed"><span>2</span></li>
                                            <li class="active"><span>3</span></li>

                                        </ul>
                                    </div>
                                    <div class="register-steps-form">

                                        <div class="regiseter-mobile-otp">
                                            <h2 class="cmn-blk-hd text-success">Registration Successfully</h2>

                                            <p>Greetings from CNet Technologies!</p>
                                            <p>Thank You for Signing Up! We are delighted to Inform that you have been Successfully Registered with CNet Technologies.</p>
                                            <p>You can now explore across the various Features, Tools & Services designed just for you!</p>


                                        </div>

                                        <div class="col-lg-12">
                                            <div class="text-center">
                                                <a class="cmn-btn" @Html.ActionLink("Login", "loginUser") <br>
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
    
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

